# Example secrets file - DO NOT COMMIT THIS WITH REAL VALUES
# Create actual secrets using: kubectl create secret generic docvault-secrets --from-literal=openrouter_api_key=your_key
apiVersion: v1
kind: Secret
metadata:
  name: docvault-secrets
type: Opaque
stringData:
  openrouter_api_key: "your-openrouter-api-key-here"
  anthropic_api_key: "your-anthropic-api-key-here"
  aws_access_key_id: "your-aws-access-key-id"
  aws_secret_access_key: "your-aws-secret-access-key"

